{
    "name": "novicell-frontend",
    "version": "5.0.0",
    "description": "Webpack and NPM scripts",
    "private": true,
    "scripts": {
        "fractal": "fractal start --sync",
        "fonts": "node configurations/copyFiles -i assets/fonts/* -o dist/fonts",
        "images": "node configurations/imagemin.js",
        "styles": "postcss src/_base/master.css --dir dist/css --ext min.css --config configurations/postcss.config.js",
        "sprites": "node configurations/sprite.js",
        "stylelint": "stylelint --help",
        "webpack": "webpack --config configurations/webpack.config.js",
        "watch:styles": "postcss src/_base/master.css --dir dist/css --ext min.css --config configurations/postcss.config.js --watch --verbose",
        "watch:webpack": "webpack --config configurations/webpack.config.js --watch",
        "build": "npm-run-all -l sprites images styles fonts webpack",
        "dev": "npm-run-all -p -r watch:webpack watch:styles fractal"
    },
    "fractal": {
        "main": "fractalfile.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.6",
        "@babel/preset-env": "^7.1.6",
        "@frctl/fractal": "^1.1.7",
        "babel-loader": "^8.0.4",
        "cssnano": "^4.1.7",
        "eslint": "5.10.0",
        "eslint-loader": "2.1.1",
        "handlebars-layouts": "3.1.4",
        "imagemin": "6.0.0",
        "npm-run-all": "4.1.5",
        "postcss": "7.0.6",
        "postcss-cli": "^6.0.1",
        "postcss-import": "^12.0.1",
        "postcss-nested": "^4.1.0",
        "postcss-preset-env": "^6.4.0",
        "postcss-reporter": "^6.0.0",
        "stylelint": "9.9.0",
        "svg-sprite": "1.5.0",
        "uglifyjs-webpack-plugin": "2.0.1",
        "webpack": "^4.26.0",
        "webpack-bundle-analyzer": "3.0.3",
        "webpack-cli": "^3.1.2"
    },
    "dependencies": {
        "flickity": "^2.1.2",
        "parallax-js": "3.1.0",
        "priority-nav": "1.0.13",
        "vanilla-lazyload": "8.17.0",
        "vanilla-text-mask": "5.1.1"
    }
}
