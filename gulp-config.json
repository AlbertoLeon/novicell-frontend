{
    "debug": false,
    
    "tasks": ["build", "styles", "scripts", "images", "icons", "watch"],
    "buildTasks": ["styles", "scripts", "images", "icons"],
    "components": ["map"],

    "styles": {
        "baseDir": "less",
        "dist": "css/compiled",
        "vendorPrefixes": ["last 2 version", "safari 5", "ie 8", "ie 9", "opera 12.1", "ios 6", "android 4"]
    },

    "scripts": {
        "baseDir": "scripts",
        "dist": "scripts/compiled"
    },

    "images": {
        "baseDir": "images",
        "dist": "images/compiled",
        "fileFormats": ["jpg", "png"],
        "optimizationLevel": 5,
        "progressive": true,
        "interlaced": true
    },

    "icons": {
        "baseDir": "images/icons",
        "dist": "images/icons/compiled"
    }
}